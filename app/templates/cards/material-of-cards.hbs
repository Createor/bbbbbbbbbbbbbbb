{{#paper-content classNames="md-whiteframe-z1 list-demo"}}
  {{#paper-list}}
    {{#each listData}}
      {{#paper-item}}
        {{#paper-tile-left}}
          {{x-card-image src=card.imageUrl class="face"}}
        {{/paper-tile-left}}

        {{#paper-tile-content}}
          <h3>{{card.name}}</h3>
          <h4>zach</h4>
          <p>
            {{card.description}}
          </p>
        {{/paper-tile-content}}
      {{/paper-item}}
      {{paper-divider}} {{! OR {{paper-divider inset=true}} }}
    {{/each}}
  {{/paper-list}}
{{/paper-content}}
