{{#if joinableGames.length}}
  {{#paper-content classNames="md-whiteframe-z1"}}
    {{#paper-list}}
      {{#each joinableGames as |game|}}
        {{game-listing
          game=game
          currentUserId=session.user.id
          deleteGame="deleteGame"}}
        {{paper-divider}}
      {{/each}}
    {{/paper-list}}
  {{/paper-content}}
{{else}}
  <h3>No games currently in progress.</h3>
{{/if}}
